<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_enabled="false" android:state_pressed="true">
        <bitmap android:src="@drawable/ic_0" />
    </item>
    <item android:state_enabled="false">
        <bitmap android:src="@drawable/ic_1" />
    </item>
    <item android:state_pressed="true" android:id="@id/pressed">
        <bitmap android:src="@drawable/ic_1" />
    </item>
    <item android:id="@id/not_pressed">
        <bitmap android:src="@drawable/ic_0" />
    </item>

    <transition android:toId="@id/pressed" android:fromId="@id/not_pressed">
        <animation-list>
			<item android:duration="25">
                <bitmap android:src="@drawable/ic_0" />
            </item>
			<item android:duration="25">
                <bitmap android:src="@drawable/ic_1" />
            </item>
			<item android:duration="25">
                <bitmap android:src="@drawable/ic_2" />
            </item>
			<item android:duration="25">
                <bitmap android:src="@drawable/ic_3" />
            </item>
			<item android:duration="30">
                <bitmap android:src="@drawable/ic_4" />
            </item>
			<item android:duration="30">
                <bitmap android:src="@drawable/ic_5" />
            </item>
			<item android:duration="30">
                <bitmap android:src="@drawable/ic_6" />
            </item>
			<item android:duration="50">
                <bitmap android:src="@drawable/ic_7" />
            </item>
			<item android:duration="50">
                <bitmap android:src="@drawable/ic_8" />
            </item>
			<item android:duration="50">
                <bitmap android:src="@drawable/ic_9" />
            </item>
			<item android:duration="50">
                <bitmap android:src="@drawable/ic_10" />
            </item>
			<item android:duration="50">
                <bitmap android:src="@drawable/ic_11" />
            </item>
			<item android:duration="50">
                <bitmap android:src="@drawable/ic_12" />
            </item>
			<item android:duration="50">
                <bitmap android:src="@drawable/ic_13" />
            </item>
			<item android:duration="50">
                <bitmap android:src="@drawable/ic_14" />
            </item>
			<item android:duration="50">
                <bitmap android:src="@drawable/ic_15" />
            </item>
			<item android:duration="50">
                <bitmap android:src="@drawable/ic_16" />
            </item>
			<item android:duration="60">
                <bitmap android:src="@drawable/ic_17" />
            </item>
			<item android:duration="50">
                <bitmap android:src="@drawable/ic_18" />
            </item>
			<item android:duration="50">
                <bitmap android:src="@drawable/ic_19" />
            </item>
			<item android:duration="70">
                <bitmap android:src="@drawable/ic_20" />
            </item>
        </animation-list>
    </transition>
    <transition android:toId="@id/not_pressed" android:fromId="@id/pressed" >
        <animation-list>
			<item android:duration="30">
                <bitmap android:src="@drawable/ic_20" />
            </item>
			<item android:duration="30">
                <bitmap android:src="@drawable/ic_22" />
            </item>
			<item android:duration="30">
                <bitmap android:src="@drawable/ic_23" />
            </item>
			<item android:duration="30">
                <bitmap android:src="@drawable/ic_24" />
            </item>
			<item android:duration="30">
                <bitmap android:src="@drawable/ic_25" />
            </item>
			<item android:duration="30">
                <bitmap android:src="@drawable/ic_26" />
            </item>
			<item android:duration="30">
                <bitmap android:src="@drawable/ic_27" />
            </item>
			<item android:duration="30">
                <bitmap android:src="@drawable/ic_28" />
            </item>
			<item android:duration="30">
                <bitmap android:src="@drawable/ic_29" />
            </item>
			<item android:duration="30">
                <bitmap android:src="@drawable/ic_30" />
            </item>
			<item android:duration="30">
                <bitmap android:src="@drawable/ic_31" />
            </item>
			<item android:duration="25">
                <bitmap android:src="@drawable/ic_0" />
            </item>
        </animation-list>
    </transition>
</animated-selector>

